#Advanced number guessing


guess = (input('Guess a number between 1-10'))


import random

actual = random.randint(1,10)

if (actual == guess):
    print('you got it!!!')

elif (guess != actual):
    newguess = input('nope, guess again')

if (actual == newguess):
    print('good job!')

else:
    print('nope')










